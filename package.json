{
    "name": "profile",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "description": "My Portfolio - Abhijit Kumar Jha",
    "author": "Abhijit Kumar Jha",
    "scripts": {
        "clean": "npm cache clean --force && rm -rf node_modules",
        "dev": "vite",
        "dev-exposed": "vite --host",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "lint-fix": "eslint . --fix",
        "lint-style": "stylelint '**/*.scss --ignore-path .gitignore",
        "lint-style-fix": "stylelint '**/*.scss --fix --ignore-path .gitignore",
        "preview": "npm run build && vite preview",
        "test": "jest",
        "test-filter": "jest -t",
        "test-watch": "jest --watch"
    },
    "engines": {
        "node": ">=24.12.0"
    },
    "dependencies": {
        "i18next": "^25.8.8",
        "react": "^19.2.0",
        "react-bootstrap": "^2.10.10",
        "react-dom": "^19.2.0",
        "react-i18next": "^16.5.4",
        "react-router": "^7.13.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@stylistic/eslint-plugin": "^5.8.0",
        "@stylistic/stylelint-plugin": "^5.0.1",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.1",
        "babel-plugin-react-compiler": "^1.0.0",
        "eslint": "^9.39.1",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-no-unsanitized": "^4.1.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "eslint-plugin-sonarjs": "^3.0.7",
        "globals": "^16.5.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "jest-fetch-mock": "^3.0.3",
        "sass": "^1.97.3",
        "stylelint": "^17.3.0",
        "stylelint-config-standard-scss": "^17.0.0",
        "stylelint-order": "^7.0.1",
        "ts-jest": "^29.4.6",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.48.0",
        "vite": "^7.3.1"
    }
